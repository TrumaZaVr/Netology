<?php
$x = rand(0,30);
    echo "Число пользователя ".$x;
echo '<br>';
$a = 9;
$b = 2;

    if ($a>$x) {
        echo "Задуманное число НЕ входит в числовой ряд";
    }
    elseif($a==$x){
        echo "Задуманное число входит в числовой ряд";
    }
    elseif($a!=$x){

        $y = $a;
        $a = $a + $b;
        $b = $y;
       echo "Число ".$b. " Входит в числовой ряд";
    }
